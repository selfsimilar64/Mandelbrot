<?xml version="1.0" encoding="utf-8"?>
<com.selfsimilartech.fractaleye.InterceptableLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/texture_shape_param_display"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:animateLayoutChanges="true"
    android:gravity="bottom|center_horizontal"
    android:orientation="horizontal">

    <FrameLayout
        android:layout_width="234dp"
        android:layout_height="wrap_content"
        android:animateLayoutChanges="true">

        <LinearLayout
            android:id="@+id/real_param_display"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:animateLayoutChanges="true"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="6dp"
            android:paddingStart="10dp"
            android:paddingTop="4dp"
            android:paddingEnd="10dp"
            android:paddingBottom="6dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingStart="10dp"
                android:paddingEnd="15dp">

                <ImageView
                    android:id="@+id/real_param_icon"
                    android:layout_width="30dp"
                    android:layout_height="match_parent"
                    android:padding="5dp"
                    android:scaleType="fitCenter"
                    app:srcCompat="@drawable/detail" />

                <TextView
                    android:id="@+id/real_param_name"
                    style="@style/ParamLabel"
                    android:gravity="center"
                    android:text="@string/detail" />

                <EditText
                    android:id="@+id/real_param_value"
                    style="@style/EditNumberDark"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="10dp"
                    android:gravity="center_vertical|end"
                    android:minWidth="46dp"
                    android:text="255"
                    android:textSize="16sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal"
                android:visibility="gone">

                <ImageButton
                    android:id="@+id/decrease_button"
                    style="@style/FloatingButton"
                    android:layout_height="48dp"
                    android:layout_marginEnd="0dp"
                    android:background="@drawable/menu_button_ripple"
                    app:srcCompat="@drawable/param_decrease" />

                <com.selfsimilartech.fractaleye.SensitivityButton
                    android:id="@+id/sensitivityButton"
                    android:layout_width="36dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/sensitivity_button_ripple"
                    app:srcCompat="@drawable/sensitivity_med" />

                <ImageButton
                    android:id="@+id/increase_button"
                    style="@style/FloatingButton"
                    android:layout_height="48dp"
                    android:layout_marginEnd="0dp"
                    android:background="@drawable/menu_button_ripple"
                    app:srcCompat="@drawable/param_increase" />

            </LinearLayout>

            <com.selfsimilartech.fractaleye.VerticalSeekBar
                android:id="@+id/seekBar"
                android:layout_width="match_parent"
                android:layout_height="26dp"
                android:max="1000"
                android:paddingStart="6dp"
                android:paddingEnd="6dp"
                android:paddingBottom="6dp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/complex_param_display"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingStart="2dp"
            android:paddingTop="10dp"
            android:paddingEnd="2dp"
            android:paddingBottom="10dp"
            android:visibility="gone">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:gravity="center"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/complex_param_icon"
                        android:layout_width="26dp"
                        android:layout_height="match_parent"
                        android:padding="3dp"
                        android:scaleType="fitCenter"
                        app:srcCompat="@drawable/seed" />

                    <TextView
                        android:id="@+id/complex_param_name"
                        style="@style/ParamLabel"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_horizontal"
                        android:gravity="center"
                        android:text="@string/position" />
                </LinearLayout>

                <include
                    android:id="@+id/sensitivity"
                    layout="@layout/continuous_sensitivity_layout"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1" />
            </LinearLayout>

            <View
                android:id="@+id/divider22"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_marginStart="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="6dp"
                android:layout_marginBottom="5dp"
                android:background="@color/divider" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/complex_param_value1"
                        style="@style/EditNumberDark"
                        android:layout_width="85dp"
                        android:layout_height="match_parent"
                        android:imeOptions="actionNext"
                        android:text="7.192214" />

                    <ToggleButton
                        android:id="@+id/uLock"
                        style="@style/LockDark"
                        android:layout_width="26dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="0dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/complex_param_value2"
                        style="@style/EditNumberDark"
                        android:layout_width="85dp"
                        android:layout_height="match_parent"
                        android:text="-0.540185" />

                    <ToggleButton
                        android:id="@+id/vLock"
                        style="@style/LockDark"
                        android:layout_width="26dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="0dp" />
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </FrameLayout>


</com.selfsimilartech.fractaleye.InterceptableLinearLayout>